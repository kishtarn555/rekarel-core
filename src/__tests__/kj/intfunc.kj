class program {
    int func() {
        return 5;
    }
    int otherFunc(n) {
        if (iszero(n)) {
            return 40;
        }
        return succ(n, 2);
    }
    program() {
        iterate(func()) {
            putbeeper();
        }
        iterate(otherFunc(7)) {
            move();
        }
        iterate(otherFunc(0)) {
            putbeeper();
        }

        otherFunc(3);
    }    
}