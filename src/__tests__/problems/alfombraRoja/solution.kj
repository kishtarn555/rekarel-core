import rekarel.globals;
class program {

	int dfs() {
		if (frontIsBlocked && leftIsBlocked && rightIsBlocked) {
			return 1;
		}
		iterate(3) turnleft();
		iterate(3) {
			if (frontIsClear) {
				move();
				iterate(dfs()) {
					iterate(2) turnleft();
					move();
					putbeeper();
					iterate(2) turnleft();
					move();
				}
				iterate(2) turnleft();
				move();
				iterate(2) turnleft();
			}
			turnleft();
		}
		iterate(2) turnleft();
		return beepersOnFloor;
	}
	program () {
		dfs();
		turnoff();
	}
}