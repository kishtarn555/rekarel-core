{
  "name": "@rekarel/core",
  "version": "1.2.1",
  "description": "Core functions for the language Karel",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx rollup --config --bundleConfigAsCjs",
    "jison_java": "npx jison .\\gramaticas\\kareljava.jison -o .\\src\\kareljava.js  && npx gulp process-jison-java",
    "jison_java2pascal": "npx jison .\\gramaticas\\java2pascal.jison -o .\\src\\java2pascal.js  && npx gulp process-jison-java2pascal",
    "jison_pascal2java": "npx jison .\\gramaticas\\pascal2java.jison -o .\\src\\pascal2java.js && npx gulp process-jison-pascal2java"
  },
  "keywords": [
    "karel"
  ],
  "author": "kishtarn",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "gulp": "^5.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-replace": "^1.1.4",
    "jison": "^0.4.17",
    "rollup": "^4.19.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "repository": "github:kishtarn555/rekarel-core"
}
